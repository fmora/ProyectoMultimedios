package com.proyectom;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.PreparedStatement;
import java.sql.SQLException;
import java.util.logging.Level;
import java.util.logging.Logger;

public class InsertTravelDB {
	public static void main (viaje Viaje){
		Connection con = null;
		PreparedStatement pst = null;
		
		String url = "jdbc:mysql://localhost:3306/carpooling";
		String user = "app";
		String password = "fernando";
		
		try{
			con = DriverManager.getConnection(url,user,password);
			
			pst = con.prepareStatement("INSERT INTO viaje(idconductor,idorigen,iddestino,fecha,hora,ptopartida,ptopartidagps,ptollegada,ptollegadagps) VALUES (? ? ? ? ? ? ? ? ?)");
			pst.setInt(1,Viaje.getidConductor());
			pst.setInt(2,Viaje.getidOrigen());
			
			
		}catch(SQLException ex){
			Logger lgr = Logger.getLogger(PreparedStatement.class.getName());
			lgr.log(Level.SEVERE, ex.getMessage(),ex);
		}
	}
}
